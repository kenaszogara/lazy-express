const express = require("express");
const router = express.Router();
const {
  findById,
  create,
  update,
  destroy,
  findAll,
} = require("./../controllers/$$CONTROLLER_NAME$$");

router.get("/", findAll);
router.get("/:id", findById);
router.post("/", create);
router.put("/:id", update);
router.delete("/:id", destroy);

module.exports = router;
